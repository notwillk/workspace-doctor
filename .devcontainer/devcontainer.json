{
	"name": "workspace-doctor",
	"build": {
		"dockerfile": "Dockerfile",
		"context": "."
	},
	"features": {
		"ghcr.io/devcontainers/features/docker-in-docker:2": {}
	},
	"containerEnv": {
		"REPO_PATH": "${containerWorkspaceFolder}"
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"github.vscode-github-actions",
				"golang.go",
				"skellock.just"
			]
		}
	},
	"remoteUser": "vscode"
}
